{"id": 509, "questionCode": "06060502", "questionTemplateId": 2, "noteDisplayTypeCode": "XNA", "questionFrameDisplayTypeCode": "XNA", "answerTextDisplayTypeCode": "ALWAYS", "questionTextDisplayTypeCode": "XNA", "questionText": "Jste řidičem vozidla z výhledu. Ve vyobrazené dopravní situaci:", "explanationNote": null, "maxNoOfDisplays": -1, "maxPostDisplayTime": -1, "basketScopeId": 12, "basketScopeOrder": 0, "pointsCount": 4, "validFrom": "2006-06-17T00:00:00", "validTo": "2099-12-31T00:00:00", "answersOrder": "0000", "questionAnswers": [{"id": 31450, "answerText": "Smíte odbočit vlevo, aniž byste někomu musel(a) dávat přednost.", "mediaContent": null, "sortOrderNumber": 1, "isCorrect": false}, {"id": 31451, "answerText": "Nesmíte odbočit vlevo, neboť zelený plný kruhový světelný signál \"Volno\" umožňuje projet křižovatku pouze v přímém směru.", "mediaContent": null, "sortOrderNumber": 2, "isCorrect": false}, {"id": 31452, "answerText": "Smíte odbočit vlevo, ale musíte dát přednost chodcům přecházejícím ve volném směru po přechodu pro chodce.", "mediaContent": null, "sortOrderNumber": 3, "isCorrect": true}], "mediaContent": {"printMediaName": "Q_W_509.mp4", "mediaUrl": "/binary_content_storage/Q_W_509.mp4", "mediaDataHash": "680329bbf617b460459925b3e9cc9723639e98e869cc5a69926dd0388729c3e6", "mediaFormatCode": "video_mp4", "isFullScreen": false}}